.tpv-tab:hover{
  background-color: $tpv-lightgray;
}
.tpv-tab-active {
  background-color: #ffffff;
  border-left: 0px solid transparent;
}
.tpv-tab-active:hover {
  background-color: #ffffff;
  cursor: default;
