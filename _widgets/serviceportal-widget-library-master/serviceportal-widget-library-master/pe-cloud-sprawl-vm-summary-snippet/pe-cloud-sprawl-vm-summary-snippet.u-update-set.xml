<name>sp_widget_c0e4ccd113ed3600d15e3d7ed144b094</name>
<payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update table="sp_widget"&gt;&lt;sp_widget action="INSERT_OR_UPDATE"&gt;&lt;client_script&gt;&lt;![CDATA[function SummaryDataController(spUtil) {
  var c = this;

  c.$onInit = function() {
    getDonutChart();
  };

  function getDonutChart() {
    spUtil.get('pe-donut-chart-wizard', {
      primary_color: c.data.chart.primary_color,
      background_color: c.data.chart.background_color,
      chart_width: c.data.chart.chart_width,
      font_size: c.data.chart.font_size,
      font_weight: c.data.chart.font_weight,
      chart_data: c.data.chart_data
    }).then(function(response) {
      c.data.donutChartWizardWidget = response;
    });
  }
}]]&gt;&lt;/client_script&gt;&lt;controller_as&gt;c&lt;/controller_as&gt;&lt;css&gt;.vms {
  overflow: hidden;
  margin-bottom: 10px;
  background: #f9f9f9;
  display: flex;
  &amp;amp;:after {
    content: "";
    display: table;
  }
  .color-red {
    color: #ff402c !important;
  }
  .arrow-up {
    margin-right: 3px;
    width: 0;
    height: 0;
    border-left: 7px solid transparent;
    border-right: 7px solid transparent;
    border-bottom: 7px solid #70be5a;
  }
  .flex-it {
    display: flex;
    align-items: center;
    justify-content: center;
    color: #70be5a;
    p {
      font-size: 12px;
      font-weight: 600;
    }
  }
  .each-box {
    width: 33.3%;
    padding: 10px 10px 20px;
    float: left;
    text-align: center;
    .bottom-part {
      margin-left: -7px;
      margin-right: -7px;
    }
    .top-text {
      height: 34px;
      display: flex;
      align-items: flex-end;
      justify-content: center;
      margin-bottom: 8px;
    }
    .top-box {
      height: 65px;
      display: flex;
      align-items: flex-start;
      justify-content: center;
    }
    p {
      margin-bottom: 0px;
    }
    .sub-title {
      font-size: 12px;
      color: #9f9f9f;
      text-transform: uppercase;
      font-weight: bold;
    }
    .top-title {
      font-size: 12px;
      color: #65666a;
      text-transform: uppercase;
      font-weight: bold;
    }
    .middle-big-text {
      font-size: 38px;
      color: #65666a; //font-weight: bold;
    }
  }
  .middle-box {
    border-right: 1px solid #ddd;
    border-left: 1px solid #ddd;
  }
}&lt;/css&gt;&lt;data_table&gt;sp_instance&lt;/data_table&gt;&lt;demo_data/&gt;&lt;description/&gt;&lt;docs/&gt;&lt;field_list/&gt;&lt;has_preview&gt;true&lt;/has_preview&gt;&lt;id&gt;pe-summary-data-snippet&lt;/id&gt;&lt;internal&gt;false&lt;/internal&gt;&lt;link/&gt;&lt;name&gt;PE Summary Data Snippet&lt;/name&gt;&lt;option_schema/&gt;&lt;public&gt;false&lt;/public&gt;&lt;roles/&gt;&lt;script&gt;&lt;![CDATA[(function() {
  data.card = {
    filter: 'all',
    donut_data: {
      total: 100,
      completed: 85,
      display_value: '21%',
      sub_title: 'NONCOMPLIANT spend',
      bottom_text: '$50k UNACCOUNTED'
    },
    second_box: {
      top_text: 'savings',
      middle_text: '$69k',
      sub_title: '47 RECLAIMED VMs'
    },
    third_box: {
      big_text: '78%',
      sub_title: 'UTILIZATION RATE',
      sub_sub_title: '3% M/M'
    }
  };

  data.chart = {
    primary_color: '#e74c3c',
    background_color: '#9b9b9b',
    chart_width: '60',
    font_size: '18px',
    font_weight: '600'
  };

  data.chart_data = {
    label: '21%',
    current: 1,
    total: 5
  };
})();]]&gt;&lt;/script&gt;&lt;servicenow&gt;false&lt;/servicenow&gt;&lt;sys_class_name&gt;sp_widget&lt;/sys_class_name&gt;&lt;sys_created_by&gt;fabio.petito&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2017-03-15 17:13:30&lt;/sys_created_on&gt;&lt;sys_customer_update&gt;true&lt;/sys_customer_update&gt;&lt;sys_id&gt;c0e4ccd113ed3600d15e3d7ed144b094&lt;/sys_id&gt;&lt;sys_mod_count&gt;36&lt;/sys_mod_count&gt;&lt;sys_name&gt;PE Summary Data Snippet&lt;/sys_name&gt;&lt;sys_package display_value="Global" source="global"&gt;global&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_replace_on_upgrade&gt;false&lt;/sys_replace_on_upgrade&gt;&lt;sys_scope display_value="Global"&gt;global&lt;/sys_scope&gt;&lt;sys_update_name&gt;sp_widget_c0e4ccd113ed3600d15e3d7ed144b094&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;steven.gregory&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2017-06-05 22:23:12&lt;/sys_updated_on&gt;&lt;template&gt;&lt;![CDATA[&lt;div class="vms"&gt;
  &lt;div class="each-box unaccounted-box" ui-sref="unaccountedvm"&gt;
    &lt;div class="top-text"&gt;
      &lt;p class="sub-title"&gt;{{::data.card.donut_data.sub_title}}&lt;/p&gt;
    &lt;/div&gt;
    &lt;div class="top-box"&gt;
      &lt;sp-widget widget="c.data.donutChartWizardWidget"&gt;&lt;/sp-widget&gt;
    &lt;/div&gt;
    &lt;p class="top-title bottom-part"&gt;{{::data.card.donut_data.bottom_text}}&lt;/p&gt;
  &lt;/div&gt;
  &lt;div class="each-box middle-box" ui-sref="vmsreclaimed({filter:data.filter})"&gt;
    &lt;div class="top-text"&gt;
      &lt;p class="sub-title"&gt;{{::data.card.second_box.top_text}}&lt;/p&gt;
    &lt;/div&gt;
    &lt;div class="top-box"&gt;
      &lt;p class="middle-big-text"&gt;{{::data.card.second_box.middle_text}}&lt;/p&gt;
    &lt;/div&gt;
    &lt;p class="top-title"&gt;{{::data.card.second_box.sub_title}}&lt;/p&gt;
  &lt;/div&gt;
  &lt;div class="each-box" ui-sref="utilization"&gt;
    &lt;div class="top-text"&gt;
      &lt;p class="sub-title"&gt;{{::data.card.third_box.sub_title}}&lt;/p&gt;
    &lt;/div&gt;
    &lt;div class="top-box"&gt;
      &lt;p class="middle-big-text color-red"&gt;{{::data.card.third_box.big_text}}&lt;/p&gt;
    &lt;/div&gt;
    &lt;div class="flex-it"&gt;
      &lt;div class="arrow-up"&gt;&lt;/div&gt;
      &lt;p&gt;{{::data.card.third_box.sub_sub_title}}&lt;/p&gt;
    &lt;/div&gt;
  &lt;/div&gt;
&lt;/div&gt;]]&gt;&lt;/template&gt;&lt;/sp_widget&gt;&lt;/record_update&gt;</payload>
