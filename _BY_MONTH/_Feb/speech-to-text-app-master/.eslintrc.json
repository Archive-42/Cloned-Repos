{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:import/recommended"],
    "settings": {
        "react": {
            "pragma": "React",
            "version": "16.6.3"
        }
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "import",
        "react"
    ],
    "rules": {
        "react/display-name": [0],
        "no-unused-vars": 0,
        "no-console": 0,
        "max-len": [1, 120, 2, {
            "ignoreComments": true
        }],
        "prop-types": [0],
        "indent": [
            0
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            0
        ],
        "semi": [
            0
        ]
    }
}