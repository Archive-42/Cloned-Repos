<% if(type == 0) { %>
<div class="page right <%- bg %>">
  <div class="eptitle"><%- title %></div>
  <div class="estitle"><%- subtitle %></div>
</div>
<% } else { %>
<div class="bg-page <%- bg %>">
  <div class="right bg bg-dim page">
    <div class="eptitle"><%- title %></div>
    <div class="estitle"><%- subtitle %></div>
  </div>
</div>
<% } %>