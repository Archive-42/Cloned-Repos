    "gatsby-plugin"
  ],
  "license": "MIT",
  "peerDependencies": {
    "gatsby": ">2.0.0-alpha"
  },
  "repository": "https://github.com/freecodecamp/gatsby-source-filesystem",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
