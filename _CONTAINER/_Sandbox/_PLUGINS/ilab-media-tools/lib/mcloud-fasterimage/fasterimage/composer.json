    "homepage": "https://github.com/willwashburn/fasterimage",
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "mcloud-willwashburn/stream": "v1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "php-mock/php-mock-phpunit": "^2.3",
        "php-coveralls/php-coveralls": "^2.1"
